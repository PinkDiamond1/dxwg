<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:ex="https://dcat.example.org/"
    xmlns:time="http://www.w3.org/2006/time#"
    xmlns:w3cgeo="http://www.w3.org/2003/01/geo/wgs84_pos#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:locn="http://www.w3.org/ns/locn#"
    xmlns:geosparql="http://www.opengis.net/ont/geosparql#"
    xmlns:dcat="http://www.w3.org/ns/dcat#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Ontology rdf:about="https://dcat.example.org/temporal-properties">
    <owl:imports rdf:resource="http://www.w3.org/ns/dcat"/>
  </owl:Ontology>
  <dcat:Dataset rdf:about="https://dcat.example.org/ds127">
    <dcterms:temporal>
      <dcterms:PeriodOfTime>
        <dcat:startDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
        >2016-03-04</dcat:startDate>
      </dcterms:PeriodOfTime>
    </dcterms:temporal>
  </dcat:Dataset>
  <dcat:Dataset rdf:about="https://dcat.example.org/ds429">
    <dcterms:temporal>
      <time:ProperInterval>
        <time:hasEnd>
          <time:Instant>
            <time:inXSDgYear rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear"
            >1939</time:inXSDgYear>
          </time:Instant>
        </time:hasEnd>
        <time:hasBeginning>
          <time:Instant>
            <time:inXSDgYear rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear"
            >1914</time:inXSDgYear>
          </time:Instant>
        </time:hasBeginning>
        <rdf:type rdf:resource="http://purl.org/dc/terms/PeriodOfTime"/>
      </time:ProperInterval>
    </dcterms:temporal>
  </dcat:Dataset>
  <dcat:Dataset rdf:about="https://dcat.example.org/ds913">
    <dcat:temporalResolution rdf:datatype="http://www.w3.org/2001/XMLSchema#duration"
    >PT15M</dcat:temporalResolution>
    <dcterms:accrualPeriodicity rdf:resource="http://purl.org/cld/freq/daily"/>
  </dcat:Dataset>
  <dcat:Dataset rdf:about="https://dcat.example.org/ds348">
    <dcterms:temporal>
      <time:ProperInterval>
        <time:hasEnd>
          <time:Instant>
            <time:inXSDDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
            >2018-08-05</time:inXSDDate>
          </time:Instant>
        </time:hasEnd>
        <time:hasBeginning>
          <time:Instant>
            <time:inXSDDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
            >2016-03-04</time:inXSDDate>
          </time:Instant>
        </time:hasBeginning>
        <rdf:type rdf:resource="http://purl.org/dc/terms/PeriodOfTime"/>
      </time:ProperInterval>
    </dcterms:temporal>
  </dcat:Dataset>
  <dcat:Dataset rdf:about="https://dcat.example.org/ds257">
    <dcterms:temporal>
      <dcterms:PeriodOfTime>
        <dcat:startDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
        >2016-03-04</dcat:startDate>
        <dcat:endDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
        >2018-08-05</dcat:endDate>
      </dcterms:PeriodOfTime>
    </dcterms:temporal>
  </dcat:Dataset>
  <dcat:Dataset rdf:about="https://dcat.example.org/ds782">
    <dcat:temporalResolution rdf:datatype="http://www.w3.org/2001/XMLSchema#duration"
    >PT1H</dcat:temporalResolution>
    <dcterms:accrualPeriodicity rdf:resource="http://purl.org/cld/freq/continuous"/>
  </dcat:Dataset>
  <dcat:Dataset rdf:about="https://dcat.example.org/ds586">
    <dcterms:temporal>
      <time:ProperInterval>
        <time:hasEnd rdf:parseType="Resource">
          <time:inXSDDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
          >2018-08-05</time:inXSDDate>
        </time:hasEnd>
        <rdf:type rdf:resource="http://purl.org/dc/terms/PeriodOfTime"/>
      </time:ProperInterval>
    </dcterms:temporal>
  </dcat:Dataset>
  <dcat:Dataset rdf:about="https://dcat.example.org/ds850">
    <dcterms:temporal>
      <time:ProperInterval>
        <time:hasEnd>
          <time:Instant>
            <time:inTimePosition>
              <time:TimePosition>
                <time:numericPosition rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal"
                >251.902</time:numericPosition>
                <time:hasTRS rdf:resource="http://resource.geosciml.org/classifier/cgi/geologicage/ma"/>
              </time:TimePosition>
            </time:inTimePosition>
          </time:Instant>
        </time:hasEnd>
        <time:hasBeginning>
          <time:Instant>
            <time:inTimePosition>
              <time:TimePosition>
                <time:numericPosition rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal"
                >541.0</time:numericPosition>
                <time:hasTRS rdf:resource="http://resource.geosciml.org/classifier/cgi/geologicage/ma"/>
              </time:TimePosition>
            </time:inTimePosition>
          </time:Instant>
        </time:hasBeginning>
        <rdf:type rdf:resource="http://purl.org/dc/terms/PeriodOfTime"/>
      </time:ProperInterval>
    </dcterms:temporal>
  </dcat:Dataset>
</rdf:RDF>
