  <dcat:Dataset rdf:about="https://dcat.example.org/dataset-001">
    <dcat:theme>
      <skos:Concept rdf:about="https://dcat.example.org/accountability">
        <skos:prefLabel xml:lang="en">Accountability</skos:prefLabel>
        <skos:inScheme rdf:resource="https://dcat.example.org/themes"/>
