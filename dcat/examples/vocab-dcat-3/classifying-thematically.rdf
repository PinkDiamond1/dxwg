    xmlns:w3cgeo="http://www.w3.org/2003/01/geo/wgs84_pos#"
    xmlns:locn="http://www.w3.org/ns/locn#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:ex="https://dcat.example.org/"
