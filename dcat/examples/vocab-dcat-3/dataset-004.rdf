<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns="http://example.org/dataset-004#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcat="http://www.w3.org/ns/dcat#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Ontology rdf:about="http://example.org/dataset-004">
    <owl:imports rdf:resource="http://www.w3.org/ns/dcat"/>
  </owl:Ontology>
  <dcat:Dataset rdf:about="http://example.org/dataset-004#dataset-004">
    <dcat:distribution>
      <dcat:Distribution rdf:about="http://example.org/dataset-004#dataset-004-png">
        <dcat:mediaType rdf:resource="https://www.iana.org/assignments/media-types/image/png"/>
        <dcat:accessURL rdf:resource="http://example.org/api/figure-006"/>
        <dcat:accessService>
          <dcat:DataService rdf:about="http://example.org/dataset-004#figure-service-006">
            <dcat:servesDataset rdf:resource="http://example.org/dataset-004#dataset-004"/>
            <dcat:endpointURL rdf:resource="http://example.org/api/figure-006"/>
            <dcat:endpointDescription rdf:resource="http://example.org/api/figure-006/params"/>
            <dcterms:type rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/SpatialDataServiceType/view"/>
            <dcterms:conformsTo rdf:resource="http://example.org/apidef/figure/v1.0"/>
          </dcat:DataService>
        </dcat:accessService>
      </dcat:Distribution>
    </dcat:distribution>
    <dcat:distribution>
      <dcat:Distribution rdf:about="http://example.org/dataset-004#dataset-004-csv">
        <dcat:mediaType rdf:resource="https://www.iana.org/assignments/media-types/text/csv"/>
        <dcat:accessURL rdf:resource="http://example.org/api/table-005"/>
        <dcat:accessService>
          <dcat:DataService rdf:about="http://example.org/dataset-004#table-service-005">
            <dcat:servesDataset rdf:resource="http://example.org/dataset-004#dataset-004"/>
            <dcat:endpointURL rdf:resource="http://example.org/api/table-005"/>
            <dcat:endpointDescription rdf:resource="http://example.org/api/table-005/capability"/>
            <dcterms:type rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/SpatialDataServiceType/download"/>
            <dcterms:conformsTo rdf:resource="http://example.org/apidef/table/v2.2"/>
          </dcat:DataService>
        </dcat:accessService>
      </dcat:Distribution>
    </dcat:distribution>
  </dcat:Dataset>
</rdf:RDF>
